{ "dashboards": [
    {
        "name": "default",
        "included_reports": [
            "processes",
            "cpu",
            "network",
            "virt_network",
            "mem",
            "swap",
            "load"
        ],
        "environments": "/.*/",
        "clusters": "/.*/"
    },
    {
        "name": "apache",
        "included_reports": [
            "apache_accesses",
            "apache_processes",
            "apache_volume"
        ],
        "environments": "/.*/",
        "clusters": "/.*/",
        "hosts": "/tador/"
    },
    {
        "name": "postfix",
        "included_reports": [
            "postfix_mailqueue",
            "postfix_mailstats",
            "postfix_mailvolume"
        ],
        "environments": "/.*/",
        "clusters": "/.*/"
    },
    {
        "name": "virtual",
        "included_reports": [
            "virt_cpu",
            "virt_network",
            "virt_disk_ops"
        ],
        "environments": "/^virtual$/",
        "clusters": "/.*/"
    }
] }
